{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "mysql",
            "image": "mysql:5.7.17",
            "environment": [
                {
                    "name": "MYSQL_ROOT_PASSWORD",
                    "value": "mnbvcxzA"
                },
                {
                    "name": "MYSQL_DATABASE",
                    "value": "lagobe"
                }
            ],
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 3306,
                    "containerPort": 3306
                }
            ]
        },
        {
            "name": "apis",
            "image": "lagobetech/lgbsc-backend:1.0.0",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 8000,
                    "containerPort": 8000
                }
            ],
            "links": [
                "mysql"
            ]
        },
        {
            "name": "www",
            "image": "lagobetech/lgbsc-frontend:1.0.1",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                },
                {
                    "hostPort": 443,
                    "containerPort": 443
                }
            ],
            "links": [
                "apis"
            ]
        }
    ]
}