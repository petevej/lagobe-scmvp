{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "apis",
            "image": "lagobetech/lgbsc-backend:1.0.3",
            "essential": true,
            "memoryReservation": 128,
            "portMappings": [
                {
                    "hostPort": 8000,
                    "containerPort": 8000
                }
            ]
        },
        {
            "name": "www",
            "image": "lagobetech/lgbsc-frontend:1.0.4",
            "essential": true,
            "memoryReservation": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                },
                {
                    "hostPort": 443,
                    "containerPort": 443
                }
            ],
            "links": [
                "apis"
            ]
        }
    ]
}